# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Note {
  id: ID!
  text: String!
  done: Boolean!
  users: [User!]!
}

type User {
  id: ID!
  name: String!
}

type Query {
  notes: [Note!]!
}

input NewNote {
  text: String!
}

type Mutation {
  createNote(input: NewNote!): Note!
  signin(email: String!, password: String!): Boolean!
}

type Time {
  unixTime: Int!
  timeStamp: String!
}

type Subscription {
  currentTime: Time!
}
